<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<!--
	Installer shortcut specification 
	Carlos A. Rueda
	$Id$
-->
<shortcuts>
  <programGroup defaultName="StarSpan" location="applications"/>
  <shortcut
     name="StarSpan GUI"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\bin\starspan-gui.bat"
     commandLine=""
     description="Launch the StarSpan GUI"
     initialState="noShow">
     <createForPack name="System"/>
   </shortcut>

   <shortcut
     name="Uninstall StarSpan GUI"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
     commandLine=""
     description="StarSpan uninstaller">
     <createForPack name="System" />
   </shortcut>
   
</shortcuts>
